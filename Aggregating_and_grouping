SELECT MAX(maxtemperaturef) 
FROM weather ;

SELECT date, zip FROM weather where maxtemperaturef = 134;

SELECT
    start_station,
    COUNT(*) as station_count
FROM
    trips
GROUP by 1;

SELECT min(duration) 
FROM trips ;

SELECT start_station, end_station, start_date FROM trips where duration = 60;

SELECT end_station, AVG(duration) as dur FROM trips GROUP BY end_station